{
  "name": "@switchboard/electron-shell",
  "version": "2.0.0",
  "description": "Instanly launch multiple Dynamics demo environments",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist:win": "electron-builder --windows --publish=never",
    "dist:osx": "electron-builder --macos --publish=never"
  },
  "repository": "https://github.com/chuanqisun/switchboard",
  "keywords": [],
  "author": "Chuanqi Sun",
  "license": "MIT",
  "devDependencies": {
    "electron": "^7.1.8",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "download-chromium": "^2.2.0",
    "puppeteer-core": "^2.0.0"
  },
  "build": {
    "artifactName": "${name}-${version}.${ext}",
    "productName": "Switchboard",
    "appId": "com.switchboard.app"
  }
}
